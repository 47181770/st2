{
    "name": "deletepacks",
    "runner_type": "run-python",
    "description": "Deletes the pack from local content repository.",
    "enabled": true,
    "entry_point": "pack_mgmt/deletepacks.py",
    "parameters": {
        "packs": {
            "type": "array",
            "items": {"type": "string"}
        },
        "abs_repo_base": {
            "type": "string",
            "default": "/opt/stackstorm",
            "immutable": true
        }
    }
}

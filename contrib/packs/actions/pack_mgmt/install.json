{
    "chain": [
        {
            "name": "download",
            "ref": "packs.download",
            "params": {"packs": "{{packs}}"},
            "on-success": "reload"
        },
        {
            "name": "reload",
            "ref": "packs.load",
            "params": {},
            "on-success": "restart-sensor-container"
        },
        {
            "name": "restart-sensor-container",
            "ref": "packs.restart_component",
            "params": {"servicename": "sensor_container"}
        }
    ],
    "default": "download"
}

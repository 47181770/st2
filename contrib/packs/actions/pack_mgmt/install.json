{
    "chain": [
        {
            "name": "download",
            "ref": "core.downloadpacks",
            "params": {"packs": "{{packs}}"},
            "on-success": "reload"
        },
        {
            "name": "reload",
            "ref": "core.reload-content",
            "params": {},
            "on-success": "restart-sensor-container"
        },
        {
            "name": "restart-sensor-container",
            "ref": "core.restart-component",
            "params": {"servicename": "sensor_container"}
        }
    ],
    "default": "download"
}

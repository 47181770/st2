{
    "name": "load",
    "runner_type": "run-local",
    "description": "Action that reloads all st2 content.",
    "enabled": true,
    "entry_point": "",
    "parameters": {
        "sudo": {
            "immutable": true
        },
        "cmd": {
            "immutable": true,
            "default": "st2ctl reload --register-{{register}}"
        },
        "register": {
            "type": "string",
            "default": "norules",
            "description": "Possible options are all, actions, rules."
        }
    }
}

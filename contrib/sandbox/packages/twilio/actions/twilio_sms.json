{
    "name": "twilio_sms",
    "runner_type": "run-local-script",
    "description": "This sends a SMS using twilio.",
    "enabled": true,
    "entry_point": "send_sms.py",
    "parameters": {
        "to": {
            "type": "string",
            "description": "Recipient number in E.164 format. Example +14151234567."
        },
        "body": {
            "type": "string",
            "description": "Body of the message."
        },
        "from": {
            "type": "string",
            "description": "Your twilio 'from' number in E.164 format. Example +14151234567."
        },
        "id": {
            "type": "string",
            "description": "Twilio account sid."
        },
        "token": {
            "type": "string",
            "description": "Twilio auth token."
        },
        "user": {
            "immutable": true
        },
        "dir": {
            "immutable": true
        },
        "sudo": {
            "immutable": true
        },
        "parallel": {
            "immutable": true
        }
    },
    "required_parameters": ["to", "from", "body"]
}

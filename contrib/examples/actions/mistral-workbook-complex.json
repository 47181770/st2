{
    "name": "mistral-workbook-complex",
    "pack": "examples",
    "runner_type": "mistral-v2",
    "description": "Run a series of simulated actions.",
    "enabled": true,
    "entry_point":"workflows/mistral-workbook-complex.yaml",
    "parameters": {
        "workflow": {
            "type": "string",
            "default": "examples.mistral-workbook-complex.main",
            "immutable": true
        },
        "task": {
            "type": "string",
            "default": null,
            "immutable": true
        },
        "context": {
            "type": "object",
            "default": {},
            "immutable": true
        },
        "vm_name": {
            "type": "string",
            "required": true
        },
        "cpu_cores": {
            "type": "integer",
            "default": 1
        },
        "memory_mb": {
            "type": "integer",
            "default": 1024
        }
    }
}

{
    "name": "mistral-workbook-subflows",
    "pack": "examples",
    "runner_type": "mistral-v2",
    "description": "Test the output for a series of mistral-basic workflows.",
    "enabled": true,
    "entry_point":"workflows/mistral-workbook-subflows.yaml",
    "parameters": {
        "workflow": {
            "type": "string",
            "default": null,
            "immutable": true
        },
        "task": {
            "type": "string",
            "default": null,
            "immutable": true
        },
        "context": {
            "type": "object",
            "default": {},
            "immutable": true
        },
        "subject": {
            "type": "string",
            "default": "StackStorm"
        },
        "adjective": {
            "type": "string",
            "default": "awesome"
        }
    }
}

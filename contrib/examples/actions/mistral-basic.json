{
    "name": "mistral-basic",
    "pack": "examples",
    "runner_type": "mistral-v2",
    "description": "Run a local linux command",
    "enabled": true,
    "entry_point":"workflows/mistral-basic.yaml",
    "parameters": {
        "workflow": {
            "type": "string",
            "default": null,
            "immutable": true
        },
        "task": {
            "type": "string",
            "default": null,
            "immutable": true
        },
        "cmd": {
            "type": "string",
            "required": true
        }
    }
}
